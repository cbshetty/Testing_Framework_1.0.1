{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "product": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "productId": {
              "type": "string"
            },
            "productName": {
              "type": "string"
            },
            "productType": {
              "type": "array",
              "items": [
                {
                  "type": "string"
                }
              ]
            }
          },
          "required": [
            "productId",
            "productName"
          ]
        }
      ]
    }
  },
  "required": [
    "product"
  ]
}