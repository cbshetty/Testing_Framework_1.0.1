{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": [
    {
      "type": "object",
      "properties": {
        "ExchangeName": {
          "type": "string"
        },
        "Files": {
          "type": "array",
          "items": [
            {
              "type": "object",
              "properties": {
                "FileType": {
                  "type": "string"
                },
                "FileName": {
                  "type": "string"
                }
              },
              "required": [
                "FileType",
                "FileName"
              ]
            }
          ]
        }
      },
      "required": [
        "ExchangeName",
        "Files"
      ]
    }
  ]
}