  "properties": {
    "contract": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "instrumentType": {
              "type": "string",
              "minLength": 1
            },
            "lotSize": {
              "type": "integer",
              "minLength": 1
            },
            "symbol": {
              "type": "string",
              "minLength": 1
            }
          },
          "required": [
